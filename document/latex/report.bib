@article{enderlinEvolvingGeometricEnvironmental2018,
  title = {Evolving Geometric and Environmental Controls on {{Columbia Glacier}} Ice Dynamics},
  author = {Enderlin, E. M. and O'Neel, S. and Bartholomaus, T. C. and Joughin, I. R.},
  date = {2018-12-01},
  volume = {2018},
  pages = {C31B-02},
  url = {https://ui.adsabs.harvard.edu/abs/2018AGUFM.C31B..02E},
  urldate = {2022-01-04},
  abstract = {The response of marine-terminating glaciers to changes in atmospheric and oceanic conditions is strongly dependent on glacier geometry. Glaciers occupying deep troughs or retrograde beds are prone to rapid retreat, whereas glaciers terminating in shallow water are much less sensitive to environmental perturbations. Thus, as a glacier's geometry evolves over time, so does its sensitivity to environmental perturbations. We use detailed observations from 2012-2016 for Alaska's Columbia Glacier and its former tributary (Post Glacier) to demonstrate how the influence of environmental forcing on iceberg calving and dynamic mass loss varies with glacier geometry. The effects of geometry on ice dynamics is apparent in both intra- and inter-annual retreat patterns of the glaciers, both in terms of magnitude and timing. Although intra-annual variations in terminus position are on the order of hundreds of meters, force budget time series reveal that seasonal speed variations are driven by the evolution of the subglacial hydrologic system and associated changes in basal drag rather than terminus position change. Seasonal variations in terminus position only drive noticeable speed change when the terminus retreats from 'sticky spots' (i.e., regions of relatively high basal drag). We also observed a shift in the timing of the annual cycle of terminus advance and retreat as Columbia's terminus retreated from a shallow shoal. When the terminus was grounded on the shoal, seasonal terminus retreat coincided with atmospheric warming, suggesting a link between surface melt, submarine melting, and/or calving. As Columbia's terminus retreated into deeper water, seasonal retreat was more influenced by changes in the ice flux. Although we focus on two glaciers, the physical controls on ice dynamics investigated here are not unique to the study glaciers. Thus, we suggest that changes in the timing of seasonal retreat reflect the manner by which glacier geometry mediates the calving response to meltwater and buoyancy. Based on the agreement between our observations and recent model results, we conclude that intra-annual records of terminus position change may offer a means to identify the primary controls on frontal ablation where sufficient in situ observations are lacking.},
  eventtitle = {{{AGU Fall Meeting Abstracts}}},
  keywords = {0720 Glaciers,CRYOSPHERE,CRYOSPHEREDE: 0722 Rock glaciers,CRYOSPHEREDE: 0758 Remote sensing,CRYOSPHEREDE: 0762 Mass balance},
  annotation = {ADS Bibcode: 2018AGUFM.C31B..02E}
}

@article{fewtrellBenchmarkingUrbanFlood2011,
  title = {Benchmarking Urban Flood Models of Varying Complexity and Scale Using High Resolution Terrestrial {{LiDAR}} Data},
  author = {Fewtrell, Timothy J. and Duncan, Alastair and Sampson, Christopher C. and Neal, Jeffrey C. and Bates, Paul D.},
  date = {2011-01},
  journaltitle = {Physics and Chemistry of the Earth, Parts A/B/C},
  shortjournal = {Physics and Chemistry of the Earth, Parts A/B/C},
  volume = {36},
  number = {7-8},
  pages = {281--291},
  issn = {14747065},
  doi = {10.1016/j.pce.2010.12.011},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S1474706510002184},
  urldate = {2021-11-15},
  abstract = {This paper describes benchmark testing of a diffusive and an inertial formulation of the de St. Venant equations implemented within the LISFLOOD-FP hydraulic model using high resolution terrestrial LiDAR data. The models are applied to a hypothetical flooding scenario in a section of Alcester, UK which experienced significant surface water flooding in the June and July floods of 2007 in the UK. The sensitivity of water elevation and velocity simulations to model formulation and grid resolution are analyzed. The differences in depth and velocity estimates between the diffusive and inertial approximations are within 10\% of the simulated value but inertial effects persist at the wetting front in steep catchments. Both models portray a similar scale dependency between 50 cm and 5 m resolution which reiterates previous findings that errors in coarse scale topographic data sets are significantly larger than differences between numerical approximations. In particular, these results confirm the need to distinctly represent the camber and curbs of roads in the numerical grid when simulating surface water flooding events. Furthermore, although water depth estimates at grid scales coarser than 1 m appear robust, velocity estimates at these scales seem to be inconsistent compared to the 50 cm benchmark. The inertial formulation is shown to reduce computational cost by up to three orders of magnitude at high resolutions thus making simulations at this scale viable in practice compared to diffusive models. For the first time, this paper highlights the utility of high resolution terrestrial LiDAR data to inform small-scale flood risk management studies. Ó 2010 Elsevier Ltd. All rights reserved.},
  langid = {english},
  file = {C\:\\Users\\maxli\\Zotero\\storage\\X2GBRZUP\\Fewtrell et al. - 2011 - Benchmarking urban flood models of varying complex.pdf}
}

@article{geraldiCanHabitatRestoration2009a,
  title = {Can Habitat Restoration Be Redundant? {{Response}} of Mobile Fishes and Crustaceans to Oyster Reef Restoration in Marsh Tidal Creeks},
  author = {Geraldi, Nathan R. and Powers, Sean P. and Heck, Kenneth L. and Cebrian, Just},
  date = {2009},
  journaltitle = {MARINE ECOLOGY PROGRESS SERIES},
  volume = {389},
  pages = {171--180},
  issn = {0171-8630},
  doi = {10.3354/meps08224},
  abstract = {Expectations of ecological and fisheries benefits of habitat restoration are commonly used to justify its high financial cost; however, few empirical studies that rigorously test these expectations exist. Here, we describe the results of a study designed to quantify the effects of restoring oyster reefs in marsh tidal creeks on the mobile macrofauna. We used a before/after control/impact (BACI) design in which fishes and crustaceans within 6 tidal creeks were sampled 7 x before and 14 x after the addition of live oyster reefs to 3 of the tidal creeks. We detected no significant effects of the addition of oyster reefs on the overall fish or crustacean assemblages within the tidal creeks, and a weak positive effect on the abundance of demersal fishes. These results challenge current predictions that restoration of oyster reefs will result in an increase in mobile fauna. Our findings of no increase in mobile fauna may have resulted from the functional redundancy of a nearby biogenic habitat because reefs were placed in tidal creeks that were surrounded by dense salt marsh. We conclude that a positive correlation between mobile fauna and oyster habitat is not automatic, and that the role of the surrounding landscape is important in predicting the response of transient fish and crustaceans to oyster reef restoration.},
  orcid-numbers = {Geraldi, Nathan/0000-0002-2669-3867 Powers, Sean/0000-0002-4770-9944},
  researcherid-numbers = {Geraldi, Nathan/AAA-3645-2020},
  unique-id = {WOS:000270673100014},
  keywords = {Ecosystem services,Fish habitat,Landscape,Oyster reef,Restoration},
  file = {C\:\\Users\\maxli\\Zotero\\storage\\ERCSGQAN\\Geraldi et al. - 2009 - Can habitat restoration be redundant Response of .pdf;C\:\\Users\\maxli\\Zotero\\storage\\HSEX2KWT\\p171-180.html}
}

