@article{oneelEvolvingForceBalance2005,
  title = {Evolving Force Balance at {{Columbia Glacier}}, {{Alaska}}, during Its Rapid Retreat},
  author = {O'Neel, Shad and Pfeffer, W. Tad and Krimmel, Robert and Meier, Mark},
  date = {2005},
  journaltitle = {Journal of Geophysical Research: Earth Surface},
  volume = {110},
  number = {F3},
  issn = {2156-2202},
  doi = {10.1029/2005JF000292},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1029/2005JF000292},
  urldate = {2022-01-04},
  abstract = {Changes in driving and resistive stresses play an essential role in governing the buoyancy forces that are important controls on the speed and irreversibility of tidewater glacier retreats. We describe changes in geometry, velocity, and strain rate and present a top-down force balance analysis performed over the lower reach of Columbia Glacier. Our analysis uses new measurements and estimates of basal topography and photogrammetric surface velocity measurements made between 1977 and 2001, while assuming depth-independent strain. Sensitivity tests show that the method is robust and insensitive to small changes in the calculation parameters. Spatial distributions of ice speed show little correspondence with driving stress. Instead, spatial patterns of ice speed exhibit a nonlinear correspondence with basal drag. Primary resistance to flow comes from basal drag, but lateral drag becomes increasingly more important throughout the retreat, which may account for observed increases in speed. Maximum basal drag is always located in a prominent constriction located ∼12 km upstream from the preretreat terminus. Once the terminus retreated into deep water off the terminal moraine marking the modern maximum extent, the upstream location of this maximum basal drag helped to promote thinning and decrease effective pressure in the lower region by limiting replenishing ice flow from upstream. An increase in both ice velocity and calving resulted, initiating what appears to be an irreversible retreat.},
  langid = {english},
  keywords = {Columbia Glacier,force balance,glaciers,tidewater},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2005JF000292},
  file = {C\:\\Users\\maxli\\Zotero\\storage\\C4FW272I\\O'Neel et al. - 2005 - Evolving force balance at Columbia Glacier, Alaska.pdf;C\:\\Users\\maxli\\Zotero\\storage\\FDLSKZB8\\2005JF000292.html}
}

